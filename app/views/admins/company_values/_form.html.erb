
<%= form_with model: [:admins, @company_value] do |form| %>
  <% if @company_value.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@company_value.errors.count, "error") %> prohibited this employee from being saved:</h2>

      <ul>
        <% @company_value.errors.each do |error| %>
          <li><%= error.full_message %></li>
        <% end %>
      </ul>
    </div>
  <% end %>
  <div class="col-md-6">
    <div class="form-group">
      <%= "Id: #{@company_value.id}" %>
    </div>

    <div class="form-group">
      <%= form.label :title %>
      <%= form.text_field :title, class: "form-control" %>
    </div>

    <div class="form-group">
      <%= "Creation time: #{@company_value.created_at}" %>
    </div>

    <div class="form-group">
      <%= "Update time: #{@company_value.updated_at}" %>
    </div>

    <div class="actions">
      <%= form.submit class: "btn btn-primary" %>
    </div>
    <br>
  </div>

<% end %>
