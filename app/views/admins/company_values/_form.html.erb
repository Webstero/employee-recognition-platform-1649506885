<%= form_with model: [:admins, @company_value] do |form| %>

  <div class="col-md-6">
    <div class="form-group">
      <%= "Id: #{@company_value.id}" %>
    </div>

    <div class="form-group">
      <%= form.label :title %>
      <%= form.text_field :title, class: "form-control" %>
    </div>

    <div class="form-group">
      <%= "Creation time: #{@company_value.created_at}" %>
    </div>

    <div class="form-group">
      <%= "Update time: #{@company_value.updated_at}" %>
    </div>

    <div class="actions">
      <%= form.submit class: "btn btn-primary" %>
    </div>
    <br>
  </div>

<% end %>
