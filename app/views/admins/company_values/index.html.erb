<h1>Company value</h1>

<table class="table">
  <thead>
    <tr>
      <th>Id</th>
      <th>Title</th>
      <th>Creation time</th>
      <th>Update time</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @company_values.each do |company_value| %>
      <tr>
        <td><%= company_value.id %></td>
        <td><%= company_value.title %></td>
        <td><%= company_value.created_at%></td>
        <td><%= company_value.updated_at%></td>
        <td><%= link_to 'Edit', edit_admins_company_value_path(company_value), class: "btn btn-primary" %></td>
        <td><%= link_to 'Destroy', admins_company_value_path(company_value), method: :delete, data: { confirm: 'Are you sure?' }, class: "btn btn-danger"  %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
<%= link_to 'New Company value', new_admins_company_value_path, class: "btn btn-primary" %> &nbsp
<%= link_to 'Back', admins_root_path, class: "btn btn-secondary" %>
